# データベース設定
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/daytrading_app

# Redis設定
REDIS_URL=redis://localhost:6379

# JWT設定
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRES_IN=7d

# 暗号化設定
ENCRYPTION_KEY=your_32_character_secret_key_here

# 株価API設定
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here
YAHOO_FINANCE_API_KEY=your_yahoo_finance_api_key_here

# SBI証券API設定
SBI_API_BASE_URL=https://api.sbisec.co.jp
SBI_API_TIMEOUT=30000

# 証券会社API設定（デモ環境）
DEMO_BROKER_API_URL=https://api.demo-broker.com
DEMO_BROKER_API_KEY=your_demo_broker_api_key_here

# アプリケーション設定
NODE_ENV=development
PORT=3000

# セキュリティ設定
CORS_ORIGIN=http://localhost:3000
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
